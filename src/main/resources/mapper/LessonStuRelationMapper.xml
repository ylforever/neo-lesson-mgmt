<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neo.lesson.mapper.LessonStuRelationMapper">
    <!--插入课程数据-->
    <insert id="insertLessonStuRelation" useGeneratedKeys="true" keyColumn="id" keyProperty="relation.id">
        INSERT INTO t_student (
            lesson_code,
            student_code,
            create_user,
            create_time
        ) VALUES (
            #{relation.lessonCode},
            #{relation.studentCode},
            #{relation.createUser},
            #{relation.createTime}
        )
    </insert>
</mapper>